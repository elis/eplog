    "enquirer": "^2.3.6",
    "listr2": "^3.8.2",
    "node-localstorage": "^2.1.6",
    "term-size": "^2.2.1",
    "yaml": "^2.0.0-5"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1"
